#: zenpanel.c:28
#, c-format
msgid ""
"\n"
"zenpanel v%s : Zenwalk System Tools Manager\n"
msgstr ""
"\n"
"zenpanel v%s : Paneau de controle Zenwalk\n"

#: zenpanel.c:29
#, c-format
msgid "Usage : zenpanel [options] ...\n"
msgstr "Utilisation : zenpanel [options] ...\n"

#: zenpanel.c:36
#, c-format
msgid "[-H] : window Height\n"
msgstr "[-H] : hauteur de la fenètre\n"

#: zenpanel.c:35
#, c-format
msgid "[-W] : window Width\n"
msgstr "[-W] : largeur de la fenètre\n"

#: zenpanel.c:30
#, c-format
msgid "[-b banner] : path to the banner image\n"
msgstr "[-b bandeau] : localisation de l'image bandeau\n"

#: zenpanel.c:33
#, c-format
msgid "[-c Category] : Freedesktop Category that must match\n"
msgstr "[-c Categorie] : catégorie Freedesktop à rechercher\n"

#: zenpanel.c:34
#, c-format
msgid "[-d directory] : where to scan for desktop files\n"
msgstr "[-d répertoire] : répertoire contenant les fichiers .desktop\n"

#: zenpanel.c:32
#, c-format
msgid "[-i icon] : icon name in theme\n"
msgstr "[-i icone] : nom de l'icone dans le thème\n"

#: zenpanel.c:31
#, c-format
msgid "[-t title] : Window title\n"
msgstr "[-t titre] : titre de la fenètre\n"
